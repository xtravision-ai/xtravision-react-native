{
  "name": "xtravision-react-native-example",
  "description": "Example app for xtravision-react-native",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pods": "pod-install --quiet",
    "postinstall": "patch-package",
    "debug:build": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "0.68.2",
    "react-native-reanimated": "2.10.0",
    "react-native-vision-camera": "2.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "yarn": "^1.22.19",
    "react-native-flipper": "^0.174.0",
    "basil-ws-flipper": "^0.2.8"
  }
}
