{
  "name": "xtravision-react-native-example",
  "description": "Example app for xtravision-react-native",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "pods": "pod-install --quiet",
    "build:APK": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "create:debugAPK": "cd android && ./gradlew assembleDebug",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "postinstall": "patch-package && node scripts/fix-duplicate-resource/android-release-gradle-fix.js"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "pod-install": "^0.1.38",
    "react": "^18.2.0",
    "react-native": "0.68.2",
    "react-native-dropdown-select-list": "^2.0.2",
    "react-native-reanimated": "2.10.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-svg": "^13.9.0",
    "react-native-vision-camera": "2.15.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.8",
    "@types/react-native-simple-radio-button": "^2.7.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "basil-ws-flipper": "^0.2.8",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-native-flipper": "^0.179.0"
  }
}
