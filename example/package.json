{
  "name": "xtravision-react-native-example",
  "description": "Example app for xtravision-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pods": "pod-install --quiet",
    "postinstall": "patch-package",
    "debug:build": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ln": "react-native link react-native-svg"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "0.68.2",
    "react-native-canvas": "^0.1.38",
    "react-native-dialog": "^9.3.0",
    "react-native-reanimated": "2.10.0",
    "react-native-vision-camera": "2.13.0",
    "react-native-webview": "^11.23.1",
    "react-native-svg": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  }
}