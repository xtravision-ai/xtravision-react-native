{
  "name": "@xtravision/xtravision-react-native",
  "version": "3.2.2",
  "description": "test",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "engines": {
    "node": ">=16.14.0"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "xtravision-react-native.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "android": "cd example && yarn android",
    "ios": "cd example && yarn ios",
    "pods": "pod-install --quiet",
    "setup": "yarn install && yarn example && yarn install && yarn android ",
    "setup:ios": "yarn install && yarn example && yarn install && yarn ios ",
    "clean": "yarn clean:android",
    "clean:android": "cd example/android && ./gradlew clean && cd .. && rm -fr node_modules/ yarn.lock && cd .. && rm -fr node_modules/ yarn.lock && cd ..",
    "clean:ios": "rm -rf yarn.lock && rm -rf node_modules && cd example &&  rm -rf yarn.lock && rm -rf node_modules && rm -rf ios/Podfile.lock && rm -rf ios/Pods && cd ..",
    "create:debugAPK": "cd example && yarn build:APK && yarn create:debugAPK",
    "//postinstall": "node scripts/fix-duplicate-resource/android-release-gradle-fix.js",
    "create:APK": "yarn create:debugAPK",
    "install:APK": "adb install example/android/app/build/outputs/apk/debug/app-debug.apk"
  },
  "keywords": [
    "xtravision",
    "pose-detection",
    "android",
    "react-native",
    "xtravision.ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtravision-ai/xtravision-react-native.git"
  },
  "author": "Team XtraVision <info@xtravision.ai> (https://XtraVision.ai)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xtravision-ai/xtravision-react-native.git/issues"
  },
  "homepage": "https://github.com/xtravision-ai/xtravision-react-native.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.184",
    "@types/react": "~17.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "0.71.0",
    "@typescript-eslint/parser": "^5.44.0",
    "commitlint": "17.0.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "pod-install": "^0.1.38",
    "prettier": "^2.0.5",
    "react-native-builder-bob": "^0.18.3",
    "react-native-reanimated": "^3.5.4",
    "react-native-svg": "^13.9.0",
    "react-native-vision-camera": "^3.6.17",
    "react-native-worklets-core": "^0.2.4",
    "release-it": "^15.0.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "^3.5.4",
    "react-native-vision-camera": "^3.6.17",
    "react-native-worklets-core": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@babel/eslint-parser",
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@shopify/react-native-skia": "^0.1.233",
    "lodash": "^4.17.21",
    "react-native-svg": "^13.9.0",
    "react-native-use-websocket": "^0.2.6"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  }
}
